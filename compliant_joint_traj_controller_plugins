<library path="lib/libcompliant_joint_trajectory_controller">

  <class name="position_controllers/CompliantJointTrajectoryController"
         type="position_controllers::CompliantJointTrajectoryController"
         base_class_type="controller_interface::ControllerBase">
    <description>
      The CompliantJointTrajectoryController executes joint-space trajectories on a set of joints.
      This variant represents trajectory segments as quintic splines and sends commands to a position interface.
    </description>
  </class>

  <class name="velocity_controllers/CompliantJointTrajectoryController"
         type="velocity_controllers::CompliantJointTrajectoryController"
         base_class_type="controller_interface::ControllerBase">
    <description>
      The JointTrajectoryController executes joint-space trajectories on a set of joints.
      This variant represents trajectory segments as quintic splines and sends commands to a velocity interface.
    </description>
  </class>

  <class name="effort_controllers/CompliantJointTrajectoryController"
         type="effort_controllers::CompliantsJointTrajectoryController"
         base_class_type="controller_interface::ControllerBase">
    <description>
      The JointTrajectoryController executes joint-space trajectories on a set of joints.
      This variant represents trajectory segments as quintic splines and sends commands to an effort interface.
    </description>
  </class>

</library>
